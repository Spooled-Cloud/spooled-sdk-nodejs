# =============================================================================
# SPOOLED SDK - ENVIRONMENT VARIABLES (for testing)
# =============================================================================
# Copy this file to .env for local testing
# =============================================================================

# =============================================================================
# API CONFIGURATION
# =============================================================================

# Spooled Backend API URL
SPOOLED_API_URL=http://localhost:8080

# Spooled API Key (starts with sk_live_ or sk_test_)
SPOOLED_API_KEY=sk_test_your_api_key_here

# =============================================================================
# gRPC CONFIGURATION
# =============================================================================

# gRPC server address (host:port format)
# Default: grpc.spooled.cloud:443
SPOOLED_GRPC_ADDRESS=localhost:50051

# =============================================================================
# TESTING
# =============================================================================

# Organization ID for integration tests
TEST_ORGANIZATION_ID=

# Queue name for integration tests
TEST_QUEUE_NAME=test-queue
